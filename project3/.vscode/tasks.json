{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "gcc compile",
        "type": "shell",
        "command": "/usr/bin/gcc",
        "args": [
          "-g",
          "-o${fileBasenameNoExtension}.out",
          "${file}"
        ],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      }
    ]
  }